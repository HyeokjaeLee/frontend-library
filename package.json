{
  "name": "@hyeokjaelee/pastime-ui",
  "version": "1.5.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/types/global.d.ts",
  "packageManager": "pnpm@8.6.12",
  "keywords": [
    "react",
    "components",
    "library",
    "typescript",
    "storybook"
  ],
  "scripts": {
    "version": "npm version",
    "version:canary": "sh ./scripts/set-canary-version.sh",
    "predev": "nodemon --watch './tailwind.config.js' --exec 'node ./tailwind.gen' &",
    "dev": "storybook dev -p 6006",
    "build:lib": "tsc && vite build",
    "build:doc": "storybook build -o dist/document && touch ./dist/document/.nojekyll",
    "deploy:lib": "npm publish ./dist/library --access=public",
    "deploy:lib-canary": "pnpm run deploy:lib --tag canary",
    "deploy:doc": "gh-pages -d dist/document -t true",
    "rule:eslint": "eslint 'src/**/*.{js,ts,tsx}'",
    "rule:eslint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "rule:prettier": "prettier 'src/**/*.{json,yaml,md,js,ts,tsx,css,scss}' --check",
    "rule:prettier:fix": "prettier 'src/**/*.{json,yaml,md,js,ts,tsx,css,scss}' --write",
    "rule:stylelint": "stylelint './src/**/*.{scss,css}'",
    "rule:stylelint:fix": "stylelint './src/**/*.{scss,css}' --fix",
    "lint:all": "pnpm rule:eslint && pnpm rule:prettier && pnpm rule:stylelint",
    "lint:all:fix": "pnpm rule:eslint:fix && pnpm rule:prettier:fix && pnpm rule:stylelint:fix"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@chromatic-com/storybook": "^1.5.0",
    "@faker-js/faker": "^8.4.1",
    "@hyeokjaelee/eslint-config": "^1.3.5",
    "@hyeokjaelee/prettier-config": "^1.3.5",
    "@hyeokjaelee/stylelint-config": "^1.3.5",
    "@storybook/addon-actions": "^8.1.10",
    "@storybook/addon-docs": "^8.1.10",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/cli": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/react-vite": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/rollup-plugin-generate-package-json": "^3.2.9",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "gh-pages": "^5.0.0",
    "lodash-es": "^4.17.21",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "react-feather": "^2.0.10",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "sass": "^1.77.6",
    "storybook": "^8.1.10",
    "stylelint": "^15.11.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-static-copy": "^0.17.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
